{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ServerUrl": "wss://api.tenclass.net/xiaozhi/v1/",
  "MqttBroker": "localhost",
  "MqttPort": 1883,
  "MqttClientId": "xiaozhi_api_client",
  "MqttTopic": "xiaozhi/chat",
  "UseWebSocket": true,
  "EnableVoice": true,
  "AudioSampleRate": 16000,
  "AudioChannels": 1,
  "AudioFormat": "opus",
  "EnableTemperatureSensor": false,
  "MusicPlayer": {
    "Backend": "mpg123",
    "CacheDirectory": "",
    "Volume": 50
  }
}
