{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning",
      "Verdure.Assistant": "Debug"
    }
  },
  "AllowedHosts": "*",
  "AutoStartVoiceChat": true,
  "ServerUrl": "wss://api.tenclass.net/xiaozhi/v1/",
  "MqttBroker": "localhost",
  "MqttPort": 1883,
  "MqttClientId": "xiaozhi_api_client",
  "MqttTopic": "xiaozhi/chat",
  "UseWebSocket": true,
  "EnableVoice": true,
  "AudioSampleRate": 16000,
  "AudioChannels": 1,
  "AudioFormat": "opus",
  "EnableTemperatureSensor": false,
  "KeywordModels": {
    "ModelsPath": "ModelFiles",
    "CurrentModel": "keyword_xiaodian.table",
    "AvailableModels": [
      "keyword_xiaodian.table",
      "keyword_cortana.table"
    ]
  },
  "MusicPlayer": {
    "Backend": "mpg123",
    "CacheDirectory": "",
    "Volume": 50
  }
}
